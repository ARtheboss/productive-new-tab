{
    "name": "Productivity New Tab",
    "description": "A new tab optimized for productivity.",
    "version": "0.0.1.0",
    "manifest_version": 2,
    "permissions" : ["management", "storage", "tabs", "https://*/"],
    "chrome_url_overrides" : {
        "newtab": "html/newtab.html"
    },
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "options_page": "html/options.html",
    "browser_action": {
      "default_title": "New Tab",
      "default_popup": "html/popup.html"
    },
    "icons":{
      "128":"assets/icon.png"
    }
}